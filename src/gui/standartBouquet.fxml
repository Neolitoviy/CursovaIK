<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="438.0" styleClass="background" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainMenuController" stylesheets="@style.css">
   <children>
      <VBox alignment="CENTER" spacing="20.0" layoutX="50.0" layoutY="50.0" prefWidth="338.0">
         <Label fx:id="labelAddStandardBouquet" text="Виберіть букет для додавання" textAlignment="CENTER" styleClass="main-menu-title" wrapText="true" textFill="white"/>
         <ChoiceBox fx:id="bouquetChoiceBox" prefWidth="250.0" />
         <Button fx:id="addStandardBouquetButton" mnemonicParsing="false" onAction="#addStandarBouquet" text="Додати" styleClass="menu-button" prefWidth="250.0" />
         <Button fx:id="backToMainFromBouquetButton" mnemonicParsing="false" onAction="#backToMainFromBouquet" text="Назад" styleClass="menu-button" prefWidth="250.0" />
      </VBox>
   </children>
</AnchorPane>
